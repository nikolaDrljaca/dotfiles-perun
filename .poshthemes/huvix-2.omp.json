{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ .HostName }} âžœ",
          "foreground": "#7FBBB3",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#DBBC7F",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\udb81\udfd3{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}) ",
          "foreground": "#7FD5EA",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "(\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}) ",
          "foreground": "#9e7eff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": "<#A7C080>git(</>{{ .HEAD }}<#A7C080>) </>",
          "foreground": "#E67E80",
          "type": "git",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
